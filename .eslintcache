t, { useEffect, useState } from \"react\";\nimport Loader from \"./Loader\";\nimport { Card } from \"react-bootstrap\";\n\nconst Repo = () => {\n  const [repositories, setRepositories] = useState([]);\n  const [loading, setLoading] = useState(true);\n  const [clicked, setClicked] = useState(\"All\");\n\n  const language = [\"All\", \"javascript\", \"react\", \"python\", \"golang\"];\n\n  const handleClicked = (e) => {\n    // console.log(e.target.textContent);\n    setClicked(e.target.textContent);\n    fetchData(e.target.textContent);\n  };\n  const fetchData = (text) => {\n    setLoading(true);\n    getRepo(text)\n      .then(({ items }) => setRepositories(items))\n      .then(() => setLoading(false));\n  };\n  //function\n  const getRepo = (language) => {\n    return new Promise((resolve, reject) => {\n      fetch(\n        `https://api.github.com/search/repositories?q=stars:>1+language:${language}&sort=stars&order=desc&type=Repositories`\n      )\n        .then((res) => res.json())\n        .then((data) => resolve(data));\n    });\n  };\n\n  useEffect(() => {\n    fetchData(clicked);\n  }, [clicked]);\n  return (\n    <>\n      <h1 className=\"container\">Explore</h1>\n      <ul className=\"popular-navigation\">\n        {language.map((item, index) => {\n          return (\n            <li\n              title={item}\n              className={clicked === item ? \"clicked\" : \"\"}\n              key={index}\n              onClick={handleClicked}\n            >\n              {item}\n            </li>\n          );\n        })}\n      </ul>\n      {loading ? (\n        <Loader />\n      ) : (\n        <div className=\"grid\">\n          {repositories.map((item) => (\n            <Card style={{ width: \"18rem\" }} key={item.id} className=\"box\">\n              <Card.Img\n                style={{\n                  borderRadius: \"55%\",\n                  width: \"12rem\",\n                  marginTop: \"1.2rem\",\n                }}\n                variant=\"top\"\n                src={item.owner.avatar_url}\n              />\n              <Card.Body style={{ color: \"black\" }}>\n                <Card.Title>{item.name}</Card.Title>\n                <Card.Text>\n                  <i className=\"lni lni-user\"></i>\n                  <span> </span>\n                  {item.full_name}\n                </Card.Text>\n                <Card.Text>\n                  <i className=\"lni lni-star-filled\"></i>\n                  {item.stargazers_count}\n                </Card.Text>\n              </Card.Body>\n            </Card>\n          ))}\n        </div>\n      )}\n    </>\n  );\n};\n\nexport default Repo;\n","/home/aman/Desktop/github-wars/src/components/Loader.js",[],"/home/aman/Desktop/github-wars/src/components/Nav.js",["35"],"/home/aman/Desktop/github-wars/src/components/Home.js",[],"/home/aman/Desktop/github-wars/src/components/Battle.js",[],{"ruleId":"36","replacedBy":"37"},{"ruleId":"38","replacedBy":"39"},{"ruleId":"40","severity":1,"message":"41","line":36,"column":6,"nodeType":"42","endLine":36,"endColumn":15,"suggestions":"43"},{"ruleId":"44","severity":1,"message":"45","line":17,"column":16,"nodeType":"46","messageId":"47","endLine":17,"endColumn":37},"no-native-reassign",["48"],"no-negated-in-lhs",["49"],"react-hooks/exhaustive-deps","React Hook useEffect has a missing dependency: 'fetchData'. Either include it or remove the dependency array.","ArrayExpression",["50"],"no-script-url","Script URL is a form of eval.","Literal","unexpectedScriptURL","no-global-assign","no-unsafe-negation",{"desc":"51","fix":"52"},"Update the dependencies array to be: [clicked, fetchData]",{"range":"53","text":"54"},[1069,1078],"[clicked, fetchData]"]